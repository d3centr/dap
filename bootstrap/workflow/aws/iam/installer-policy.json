{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "eks:ListClusters"
            ],
            "Resource": [
                "arn:aws:eks:$REGION:$ACCOUNT:cluster/*"
            ],
            "Effect": "Allow"
        },
        {
            "Action": [
                "eks:DescribeCluster"
            ],
            "Resource": [
                "arn:aws:eks:$REGION:$ACCOUNT:cluster/$new-dap"
            ],
            "Effect": "Allow"
        }
    ]
}
