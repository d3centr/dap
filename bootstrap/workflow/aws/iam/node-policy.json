{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "s3:ListBucket",
                "s3:GetObject"
            ],
            "Resource": [
                "arn:aws:s3:::*-blake-$REGION-*-$ACCOUNT",
                "arn:aws:s3:::*-blake-$REGION-*-$ACCOUNT/*"
            ],
            "Effect": "Allow"
        },
        {
            "Action": [
                "ecr:InitiateLayerUpload",
                "ecr:UploadLayerPart",
                "ecr:CompleteLayerUpload",
                "ecr:PutImage",
                "s3:DeleteObject",
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:ecr:$REGION:$ACCOUNT:repository/$new-blake/*",
                "arn:aws:s3:::$new-blake-$REGION-*-$ACCOUNT/*"
            ],
            "Effect": "Allow"
        }
    ]
}
